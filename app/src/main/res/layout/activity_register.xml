<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/hasUser"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/logo"
    tools:context=".registerActivity">

    <EditText
        android:id="@+id/editTextTexttLastName"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Enter a lastName"
        android:inputType="textEmailAddress"
        android:textColorHint="#E6857474"
        app:layout_constraintBottom_toTopOf="@+id/back"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTexttFrirstName"
        app:layout_constraintVertical_bias="0.073" />

    <EditText
        android:id="@+id/editTextTexttFrirstName"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Enter a firstName"
        android:inputType="textEmailAddress"
        android:textColorHint="#5C5454"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="152dp" />

    <EditText
        android:id="@+id/editTextTextEmail"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Enter an email"
        android:inputType="textEmailAddress"
        android:textColorHint="#9F8989"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="318dp" />

    <Button
        android:id="@+id/back"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        android:text="back"
        android:textColorHint="#FF80AB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTexttFrirstName"
        app:layout_constraintVertical_bias="0.865" />

    <Button
        android:id="@+id/login_button"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@android:color/transparent"
        android:text="register"
        android:textColorHint="#E6857474"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhoneNumber"
        app:layout_constraintVertical_bias="0.538" />

    <EditText
        android:id="@+id/editTextNumberPassword"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Enter a passowrd"
        android:inputType="numberPassword"
        android:textColorHint="#E6857474"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextEmail"
        app:layout_constraintVertical_bias="0.257" />

    <EditText
        android:id="@+id/editTextPhoneNumber"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="Enter a phonenumber"
        android:inputType="numberPassword"
        android:textColorHint="#E6857474"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="389dp" />

    <Spinner
        android:id="@+id/userSpinner"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@color/white"
        android:textColor="@color/black"
        android:visibility="visible"
        app:backgroundTint="@color/material_on_surface_stroke"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="549dp"
        tools:visibility="visible" />
    <!--
        <ProgressBar
            android:id="@+id/progressBar"
            style="?android:attr/progressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.462"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/back"
            app:layout_constraintVertical_bias="0.531" />
    -->
</androidx.constraintlayout.widget.ConstraintLayout>